#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

#include <stdio.h>
#include <math.h>

int main() {
    double num1;
    int num2, base, expo;

    scanf("%lf", &num1);
    scanf("%d", &num2);
    scanf("%d", &base);
    scanf("%d", &expo);

    printf("%d\n", (int)floor(num1));
    printf("%d\n", (int)ceil(num1));
    printf("%d\n", (int)sqrt(num2));
    printf("%d\n", (int)pow(base, expo));

    return 0;
}
