#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,b,add,sub,mul,div,mod;
    scanf("%d",&a);
    scanf("%d",&b);
    add=a+b;
    sub=a-b;
    mul=a*b;
    div=a/b;
    mod=a%b;
    printf("Addition=%d\n",add);
    printf("Subtraction=%d\n",sub);
    printf("Multiplication=%d\n",mul);
    printf("Division=%d\n",div);
    printf("Modulo=%d\n",mod);
    
    return 0;
}
