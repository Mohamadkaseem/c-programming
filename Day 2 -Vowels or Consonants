#include <stdio.h>


int main() {
    char ch;
    scanf(" %c", &ch);

    if (!isalpha(ch)) {
        printf("Invalid Input\n");
    } else {
        ch = tolower(ch);
        if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
            printf("The Character %c is Vowel\n", toupper(ch));
        } else {
            printf("The Character %c is Consonant\n", toupper(ch));
        }
    }

    return 0;
}
